- comma
#include <iostream>

using namespace std;

int main()
{
    string n;
    cin >> n;
    for (int i = n.size() - 3, c = 0; i > 0; i--, c++) {
        if (c % 3 == 0) n.insert(i, ",");
    }
    cout << n;
}

